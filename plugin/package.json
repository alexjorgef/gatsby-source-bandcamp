{
  "name": "@alexjorgef/gatsby-source-bandcamp",
  "version": "0.0.1",
  "description": "Gatsby source plugin for Bandcamp",
  "main": "dist/gatsby-node.js",
  "source": "src/gatsby-node.ts",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/*",
    "gatsby-node.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexjorgef/gatsby-source-bandcamp.git",
    "directory": "package"
  },
  "keywords": [
    "bandcamp",
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "gatsby-bandcamp",
    "gatsby-source-bandcamp"
  ],
  "author": "Alexandre Ferreira <contact@alexjorgef.com>",
  "license": "MIT",
  "dependencies": {
    "@alexjorgef/bandcamp-scraper": "1.4.3"
  },
  "bugs": {
    "url": "https://github.com/alexjorgef/gatsby-source-bandcamp/issues"
  },
  "homepage": "https://github.com/alexjorgef/gatsby-source-bandcamp#readme",
  "devDependencies": {
    "@parcel/packager-ts": "^2.9.3",
    "@parcel/transformer-typescript-types": "^2.9.3",
    "del-cli": "^5.0.1",
    "gatsby": "^5.12.1",
    "gatsby-plugin-utils": "^4.12.0",
    "parcel": "^2.9.3"
  },
  "peerDependencies": {
    "gatsby": "^5.0.0-next.0"
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build --no-source-maps",
    "prepare": "npm run clean && npm run build",
    "clean": "del-cli dist/*"
  },
  "packageManager": "yarn@3.2.2"
}
